[include]

[ignore]
.*/node_modules/react-apollo/.*
.*/node_modules/apollo-client/.*
.*/node_modules/fbjs/.*
.*/node_modules/graphql/.*
.*/node_modules/eslint-plugin-jsx-a11y/.*
.*/node_modules/react-universal-component/.*
.*/node_modules/webpack-flush-chunks/.*

[options]
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
module.system=haste
module.system.node.resolve_dirname=screens
module.system.node.resolve_dirname=components
module.system.node.resolve_dirname=shared
module.name_mapper='.*\.graphql$' -> 'empty/object'