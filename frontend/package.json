{
  "private": true,
  "scripts": {
    "dev": "react-scripts dev",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:watch": "yarn run test -- --watch",
    "flow": "flow",
    "lint": "react-scripts lint",
    "lint:fix": "react-scripts lint --fix",
    "prettier": "prettier --trailing-comma all --single-quote --write",
    "format": "yarn run prettier -- 'app/**/*.js' && yarn run lint:fix",
    "persist": "node bin/update-persistgraphql",
    "introspect": "node bin/update-introspection",
    "precommit": "lint-staged && node bin/precommit",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o styleguide"
  },
  "lint-staged": {
    "linters": {
      "frontend/app/**/*.js": [
        "prettier",
        "lint:fix",
        "git add"
      ]
    },
    "gitDir": "../"
  },
  "dependencies": {
    "apollo-client": "^1.9.0-1",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "fetch-everywhere": "^1.0.5",
    "graphql-anywhere": "^4.0.1",
    "graphql-tag": "^2.5.0",
    "persistgraphql": "^0.3.11",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-apollo": "^1.4.8",
    "react-dom": "^16.1.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-tree-walker": "^2.1.3",
    "react-universal-component": "^2.6.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.4.0",
    "webpack-flush-chunks": "^1.2.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@amazee/react-scripts": "^3.0.0",
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/addon-storyshots": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "flow-bin": "^0.59.0",
    "jssha": "^2.3.1",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2",
    "sync-request": "^4.1.0"
  },
  "engines": {
    "node": "v8"
  }
}
